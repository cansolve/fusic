
.list {
  padding: 10rpx 20rpx;
  margin: 20rpx 0;
  font-size: 26rpx;
  border-radius: 5rpx;
  border-bottom: 1rpx solid #F2F2F2;
  background: #FFFFFF;
}
.list-main {
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
}
.list-main-left {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
      -ms-flex: 1;
          flex: 1;
}
.list-main-image {
  width: 200rpx;
  height: 200rpx;
}
.list-main-right {
  -webkit-box-flex: 5;
  -webkit-flex: 5;
      -ms-flex: 5;
          flex: 5;
  padding: 3rpx 5rpx;
}
.list-main-right .subTitle {
  font-size: 24rpx;
  color: #666666;
}
.list .margin-bottom-15 {
  margin-bottom: 15rpx;
}
